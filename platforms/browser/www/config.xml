<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bryanleister.typographicgrid" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>TypographyFINE2415</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="bryan.leister@ucdenver.edu" href="http://designucd.com">
        Digital Design Team at CU Denver
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashMaintainAspectRatio" value="true" />
</widget>
